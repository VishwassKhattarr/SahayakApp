{
    

    "routes": [
    {
      "src": "/api/:path*",
      "dest": "https://sahayak-app.onrender.com/api/:path*"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/scripts/:path*",
      "dest": "/src/scripts/:path*"
    },
    {
      "src": "/src/components/:path*",
      "dest": "/src/components/:path*"
    },
    {
      "src": "/pages/:path*",
      "dest": "/src/pages/:path*"
    },
    {
      "src": "/components/:path*",
      "dest": "/src/components/:path*"
    }
  ]
}